{
  "name": "@withtyped/postgres",
  "version": "0.13.0",
  "author": "Gao Sun",
  "license": "MIT",
  "type": "module",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./*": {
      "import": "./*"
    }
  },
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "prepack": "pnpm build",
    "build": "rm -rf lib/ && tsc",
    "dev": "pnpm build --watch --preserveWatchOutput --incremental",
    "dev:test": "pnpm build --sourcemap && pnpm test",
    "lint": "eslint src/",
    "test": "c8 node --test",
    "test:only": "node --test"
  },
  "devDependencies": {
    "@silverhand/eslint-config": "^5.0.0",
    "@silverhand/ts-config": "^5.0.0",
    "@types/node": "^20.12.8",
    "@types/sinon": "^17.0.3",
    "@withtyped/server": "workspace:*",
    "c8": "^7.12.0",
    "eslint": "^8.42.0",
    "lint-staged": "^13.0.4",
    "prettier": "^3.0.0",
    "sinon": "^17.0.1",
    "typescript": "^5.3.3",
    "zod": "^3.19.1"
  },
  "peerDependencies": {
    "@withtyped/server": "workspace:^0.13.4"
  },
  "eslintConfig": {
    "extends": "@silverhand",
    "rules": {
      "@typescript-eslint/padding-line-between-statements": "off"
    }
  },
  "prettier": "@silverhand/eslint-config/.prettierrc",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@silverhand/essentials": "^2.8.1",
    "@types/pg": "^8.6.5",
    "@withtyped/shared": "workspace:*",
    "pg": "^8.8.0"
  }
}
