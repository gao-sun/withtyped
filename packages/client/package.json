{
  "name": "@withtyped/client",
  "version": "0.8.2",
  "author": "Gao Sun",
  "license": "MIT",
  "type": "module",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./*": {
      "import": "./*"
    }
  },
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "prepack": "pnpm build",
    "build": "rm -rf lib/ && tsc",
    "dev": "pnpm build --watch --preserveWatchOutput --incremental",
    "dev:test": "pnpm build --sourcemap && pnpm test",
    "lint": "eslint src/",
    "test": "c8 node --test",
    "test:only": "node --test"
  },
  "devDependencies": {
    "@silverhand/eslint-config": "^5.0.0",
    "@silverhand/ts-config": "^5.0.0",
    "@types/node": "^18.11.9",
    "@types/sinon": "^10.0.13",
    "c8": "^7.12.0",
    "eslint": "^8.42.0",
    "lint-staged": "^13.0.4",
    "prettier": "^3.0.0",
    "sinon": "^14.0.2",
    "typescript": "^5.3.3",
    "zod": "^3.19.1"
  },
  "eslintConfig": {
    "extends": "@silverhand"
  },
  "prettier": "@silverhand/eslint-config/.prettierrc",
  "dependencies": {
    "@withtyped/server": "workspace:*",
    "@withtyped/shared": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}
